{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "shell:default",
    "shell:allow-open",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:webview:default"
  ]
}
